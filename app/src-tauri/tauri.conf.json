{
	"build": {
		"beforeDevCommand": "pnpm exec vite --clearScreen=false --mode development",
		"beforeBuildCommand": "pnpm exec vite build",
		"devPath": "http://localhost:1420",
		"distDir": "../dist",
		"withGlobalTauri": false
	},
	"package": {
		"productName": "otoko",
		"version": "0.0.1"
	},
	"tauri": {
		"allowlist": {
			"http": {
				"all": true,
				"request": true,
				"scope": [
					"https://r2.leermanga.xyz/*",
					"https://onlinemanga.xyz/*"
				]
			},
			"path": {
				"all": true
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "com.albizures.otoko",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"height": 600,
				"resizable": true,
				"title": "otoko",
				"width": 800
			}
		]
	}
}
